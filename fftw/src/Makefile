# Macros to link the present library
# (may be change by make.sys to the path of installed library)
LIB_FFTW=libfftw.a
LIB_FFTW_INCLUDE=-I. -I$(IDEHOME)/fftw/include

# Switch to enable actual compilation
# (may be change by make.sys to "external")
LIB_FFTW_SWITCH=internal
# Path for include
AUTO_INCLUDE=$(LIB_FFTW_INCLUDE) 

# List of used library files
AUTO_LIBS=$(LIB_FFTW) 
# Inclusion of system dependent files
include $(IDEHOME)/make.sys
# Macro (re)definitions from OPTIONS file (keyword DEFINITION)
PRECISION = $(FAUTODBL)

# Main target
# INFO print a short help
info:
	@make print_info
# INFO make local library and all executables
all:
	make loclib

# INFO make local library
loclib: libs loclib_only

loclib_only: ide_$(LIB_FFTW_SWITCH)

ide_internal: libfftw.a

ide_external: fake_external.a

fake_external.a:
	touch $(OBJ_LIB)
	touch libfftw.a
	touch fake_external.a
# Targets for executables


# Targets for libraries
# INFO make local library (without the used libraries)
libfftw.a: $(OBJ_LIB)
	$(AR) $(ARFLAGS) libfftw.a $(OBJ_LIB)
	$(RANLIB) libfftw.a

# INFO make all the used libs
libs:


# target for making Makefile
# INFO make local Makefile
make: 
	cd $(IDEHOME) ; IDE/makemake.sh fftw 

# INFO make local and libs Makefiles
allmake:
	cd $(IDEHOME) ; IDE/makemake.sh fftw 


# target for cleaning
clean_: clean
# INFO cleanup
clean: 
	- rm -f *.a *.o *.mod *.d *.pc *.pcl *.x *.F90
	if test -d  $(IDEHOME)/fftw/tmp/CVS  ; then \
	mv  $(IDEHOME)/fftw/tmp/CVS \
	$(IDEHOME)/fftw/tmp/.CVS ; fi
	- rm -rf $(IDEHOME)/fftw/tmp/*
	if test -d  $(IDEHOME)/fftw/tmp/.CVS  ; then \
	mv  $(IDEHOME)/fftw/tmp/.CVS  \
	$(IDEHOME)/fftw/tmp/CVS ; fi
	- rm -f $(IDEHOME)/fftw/bin/*.x 

tarlist: 
	cd $(IDEHOME) ; IDE/tarlist.sh fftw > fftw/tmp/tarlist

# INFO gzipped tar
tar: tarlist
	cd $(IDEHOME) ;         tar cf fftw-2.1.5p0.tar `cat fftw/tmp/tarlist`
	cd $(IDEHOME) ; gzip -9 fftw-2.1.5p0.tar

# INFO gzipped tar including used libraries and IDE
alltar: 
	cd $(IDEHOME) ;         tar cf fftw-2.1.5p0-withdeps.tar IDE CONFIG configure Makefile
	cd $(IDEHOME) ;         tar rf fftw-2.1.5p0-withdeps.tar OPTIONS
	cd $(IDEHOME) ;          tar rf fftw-2.1.5p0-withdeps.tar `cat fftw/tmp/tarlist`
	cd $(IDEHOME) ; gzip -9 fftw-2.1.5p0-withdeps.tar

