# Main target
# INFO print a short help
info:
	@make print_info
# INFO make local library and all executables
all:
	make loclib

# INFO make local library
loclib: libs loclib_only

loclib_only: ide_$(LIB_BLAS_SWITCH)

ide_internal: libblas.a

ide_external: fake_external.a

fake_external.a:
	touch $(OBJ_LIB)
	touch libblas.a
	touch fake_external.a
# Targets for executables


# Targets for libraries
# INFO make local library (without the used libraries)
libblas.a: $(OBJ_LIB)
	$(AR) $(ARFLAGS) libblas.a $(OBJ_LIB)
	$(RANLIB) libblas.a

# INFO make all the used libs
libs:


# target for making Makefile
# INFO make local Makefile
make: 
	cd $(IDEHOME) ; IDE/makemake.sh blas 

# INFO make local and libs Makefiles
allmake:
	cd $(IDEHOME) ; IDE/makemake.sh blas 


# target for cleaning
clean_: clean
# INFO cleanup
clean: 
	- rm -f *.a *.o *.mod *.d *.pc *.pcl *.x *.F90
	if test -d  $(IDEHOME)/blas/tmp/CVS  ; then \
	mv  $(IDEHOME)/blas/tmp/CVS \
	$(IDEHOME)/blas/tmp/.CVS ; fi
	- rm -rf $(IDEHOME)/blas/tmp/*
	if test -d  $(IDEHOME)/blas/tmp/.CVS  ; then \
	mv  $(IDEHOME)/blas/tmp/.CVS  \
	$(IDEHOME)/blas/tmp/CVS ; fi
	- rm -f $(IDEHOME)/blas/bin/*.x 

tarlist: 
	cd $(IDEHOME) ; IDE/tarlist.sh blas > blas/tmp/tarlist

# INFO gzipped tar
tar: tarlist
	cd $(IDEHOME) ;         tar cf blas-1.0.0p0.tar `cat blas/tmp/tarlist`
	cd $(IDEHOME) ; gzip -9 blas-1.0.0p0.tar

# INFO gzipped tar including used libraries and IDE
alltar: 
	cd $(IDEHOME) ;         tar cf blas-1.0.0p0-withdeps.tar IDE CONFIG configure Makefile
	cd $(IDEHOME) ;         tar rf blas-1.0.0p0-withdeps.tar OPTIONS
	cd $(IDEHOME) ;          tar rf blas-1.0.0p0-withdeps.tar `cat blas/tmp/tarlist`
	cd $(IDEHOME) ; gzip -9 blas-1.0.0p0-withdeps.tar

